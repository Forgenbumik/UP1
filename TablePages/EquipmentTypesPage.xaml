<Page x:Class="UP1.TablePages.EquipmentTypesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UP1.TablePages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EquipmentTypesPage" IsVisibleChanged="Page_IsVisibleChanged">

    <Grid Background="White">
        <DataGrid x:Name="GridType" AutoGenerateColumns="False">
            <DataGridTextColumn Header="Номер" Width="*" Binding="{Binding EquipmentTypeID}"/>
            <DataGridTextColumn Header="Наименование" Width="*" Binding="{Binding Name}"/>
        </DataGrid>
    </Grid>
</Page>
